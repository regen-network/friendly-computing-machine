    "regen:landManagementActions": {
        "@list": [
            {
                "schema:name": "Time Controlled Rotational Grazing",
                "schema:image": {
                    "@type": "schema:URL",
                    "@value": "https://regen-registry.s3.amazonaws.com/projects/wilmot/time-controlled-rotational-grazing.jpg"
                },
                "schema:description": "Since 2013, Wilmot Cattle Co has implemented time controlled rotational grazing at Woodburn. Effectively mimicking natural herding behaviours, time controlled rotational grazing is the practice of moving large herds of animals across the landscape, grazing pastures for very short periods of time, allowing long rest periods, enabling adequate plant recovery. Stocking rate is adjusted frequently to match the feed available and growth rate of the plant. When managed well, this grazing system increases groundcover, biomass, plant diversity, animal and plant nutrition, soil health, water infiltration and soil organic carbon."
            },
            {
                "schema:name": "Decrease Paddock Size",
                "schema:image": {
                    "@type": "schema:URL",
                    "@value": "https://regen-registry.s3.amazonaws.com/projects/wilmot/decrease-paddock-size.jpg"
                },
                "schema:description": "Decreased paddock sizes enable effective grazing at a controllable density. Woodburn has adopted a paddock subdivision program with an average paddock size of 22 hectares."
            },
            {
                "schema:name": "Increase Stock Density",
                "schema:image": {
                    "@type": "schema:URL",
                    "@value": "https://regen-registry.s3.amazonaws.com/projects/wilmot/increase-stock-density.jpg"
                },
                "schema:description": "Carefully managed, high stock densities lead to evenly grazed pastures, nutrient recycling, increases in pasture health and performance by trampling (or recycling) a higher proportion of biomass, and evenly distributing dung and urine (nutrients)."
            },
            {
                "schema:name": "Monitor Pasture Growth",
                "schema:image": {
                    "@type": "schema:URL",
                    "@value": "https://regen-registry.s3.amazonaws.com/projects/wilmot/monitor-pasture-growth.jpg"
                },
                "schema:description": "Accurate measurement of pasture growth (available feed) determines the density of animals and length of time in a paddock."
            },
            {
                "schema:name": "Manage Grazing Periods",
                "schema:image": {
                    "@type": "schema:URL",
                    "@value": "https://regen-registry.s3.amazonaws.com/projects/wilmot/manage-grazing-periods.jpg"
                },
                "schema:description": "To ensure effectively implemented grazing practices and management practices records, Woodburn utilizes the online tool <a href=\"https://www.maiagrazing.com/\" rel=\"noopener noreferrer\" target=\"_blank\">Maia Grazing</a>. Maia Grazing uses climatic data and field observations to determine pasture growth and optimal stocking rates, tracks stock movements, and ensures the stocking rate does not exceed the carrying capacity of each paddock."
            }
        ]
    },
    "regen:landStewardStoryTitle": "Grant and Tanya Wickham have managed Woodburn since 2011"
